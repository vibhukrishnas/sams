{
  "name": "sams-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace SamsMobile.xcworkspace -scheme SamsMobile -configuration Release -destination generic/platform=iOS -archivePath SamsMobile.xcarchive archive"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.6.2",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-background-job": "^2.2.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-paper": "^5.11.4",
    "react-native-permissions": "^4.1.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.27.0",
    "react-native-sound": "^0.11.2",
