# Environment Variables for SAMS Web Console

# API Configuration
API_URL=https://api.sams-monitoring.com
WS_URL=wss://api.sams-monitoring.com/ws

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_PUSH_NOTIFICATIONS=true

# Performance
MAX_DATA_POINTS=100
CHART_UPDATE_INTERVAL=1000

# Service Worker
SW_CACHE_VERSION=1
SW_DEBUG_MODE=false

# Error Reporting
SENTRY_DSN=your-sentry-dsn
LOG_LEVEL=info
